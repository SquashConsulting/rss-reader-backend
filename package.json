{
  "name": "rss-reader",
  "version": "0.1.0",
  "description": "Modern RSS Reader",
  "main": "dist/entry.js",
  "private": true,
  "scripts": {
    "presetup:repo": "npm run build",
    "preteardown:repo": "npm run build",
    "build": "tsc -p .",
    "start": "npm run build && node dist/entry.js",
    "live": "nodemon -e ts --exec \"npm run start\"",
    "reset:repo": "npm run teardown:repo && npm run setup:repo",
    "setup:repo": "node dist/repository/setup.js",
    "teardown:repo": "node dist/repository/teardown.js",
    "postbuild": "ef-tspm > /dev/null"
  },
  "bin": "./dist/entry.js",
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/joi": "^14.3.4",
    "@types/lodash.omit": "^4.5.6",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.5.0",
    "@types/yargs": "^15.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "arangojs": "^6.14.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-joi-validator": "^2.0.1",
    "joi": "^14.3.1",
    "json-api-serializer": "^2.4.2",
    "lodash.omit": "^4.5.0",
    "morgan": "^1.10.0",
    "rss-parser": "^3.8.0"
  }
}
